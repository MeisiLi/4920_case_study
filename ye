#include <stdio.h>
#include <stdlib.h>
 #include <string.h>
 #define MAX 256

 int strcompare(char str1[MAX], char str2[MAX], int len) {
     int i = 0;
     int diff = 0;
     while (i < len) {
         int c1 = str1[i];
         int c2 = str2[i];
         int dis = c1 - c2;
         if (dis == 0 || dis == 32)  diff++;
         i++;
     }
     return len - diff;
 }

 int main(int argc, char *argv[]) {
     if (argc != 2)  return 0;

     char str1[MAX], str2[MAX];
     fgets(str1, MAX, stdin);
     fgets(str2, MAX, stdin);

     int len1 = strlen(str1);
     int len2 = strlen(str2);

     int dif1 = strcompare(str1, argv[1]);
     int dif2 = strcompare(str2, argv[1]);

     if (dif1 == 0 || dif2 == 0) {
         printf("0 %s\n", argv[1]);                                                                                                             34     } else if (dif1 > dif2) {
         printf("%d %s", dif2, str2);
     } else {
         printf("%d %s", dif1, str1)
     }

     return 0;
 }
